#include <iostream>

using namespace std;
int main()
{

	int num, count = 1, count_num = 0;

	cin >> num;

	for (int i = 1; i < num; ++i)
	{

		count_num = num % i;

		if (count_num == 0) count += 1;
	}

	if (num <= 1) cout << "not simple ";

	else if (count > 2) cout << "not simple ";

	else cout << "simple ";

}

